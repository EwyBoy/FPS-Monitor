  "authors": [
    "Ewy"
  ],
  "contact": {
    "homepage": "https://github.com/EwyBoy/FPS-Monitor",
    "sources": "https://github.com/EwyBoy/FPS-Monitor",
    "issues": "https://github.com/EwyBoy/FPS-Monitor/issues"
  },

  "license": "MIT",
  "icon": "assets/fps/icon.png",

  "environment": "client",
  "entrypoints": {
